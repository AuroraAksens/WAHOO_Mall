/* pages/warehouse//house/house.wxss */
page {
    background-color: #F3F2F2;
}

.header {
    width: 100%;
    height: 90rpx;
    background-color: white;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20rpx 30rpx;
    border-top: 1rpx solid #ebebeb80;
}

.header>.gro {
    width: 18%;
    height: 100%;
    color: #DE8E6A;
    text-align: center;
    font-size: 26rpx;
    font-weight: bold;
    line-height: 60rpx;
}

.header>.search {
    width: 100%;
    height: 60rpx;
    border: 1rpx solid #c5c5c5e1;
    border-radius: 60rpx;
    overflow: hidden;
}

.header>.search>input {
    width: 100%;
    height: 100%;
    padding: 0 30rpx;
}



/* 商品模块 */
.conBody {
    width: 100%;
    border-radius: 20rpx;
    background-color: white;
    margin-top: 20rpx;
    padding: 25rpx;
}

.conBody>.top-box {
    width: 100%;
    display: flex;
    justify-content: start;
    align-items: center;
    position: relative;
}

.conBody>.top-box>image {
    width: 100rpx;
    height: 100rpx;
    border-radius: 15rpx;
}

.conBody>.top-box>.t-right {
    width: 75%;
    margin-left: 25rpx;
    display: flex;
    align-self: start;
    flex-direction: column;
}
.conBody>.top-box>.t-right>.tr-box{
    width: 100%;
    height: 50rpx;
}
.conBody>.top-box>.t-right>.tr-box>.trboxText{
    font-size: 24rpx;
    width: 160rpx;
    position: absolute;
    left: 130rpx;
    padding-top: 20rpx;
}
.conBody>.top-box>.t-right>.tr-box>.trboxTexts {
    width: 50rpx;
    font-size: 80rpx;
    position: absolute;
    right: 20rpx;
}

