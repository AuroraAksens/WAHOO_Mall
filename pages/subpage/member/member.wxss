page {
    background-color: #fff;
  }
  
  /* 顶部轮播文字 */
  .title {
    position: absolute;
    top: 30rpx;
    left: 0;
  }
  
  .title>.top {
    font-size: 30rpx;
    padding-left: 24rpx;
    color: white;
    display: flex;
    justify-content: start;
    align-items: center;
  }
  
  .title>.top>image {
    width: 25rpx;
    height: 21rpx;
    margin-right: 20rpx;
  }
  
  .title>.bottom {
    width: 230rpx;
    height: 13rpx;
    border-radius: 0 12rpx 12rpx 0;
    background-color: white;
    opacity: 0.4;
    margin-top: 10rpx;
  }
  
  /* 顶部轮播按钮 */
  .but {
    width: 160rpx;
    height: 60rpx;
    border-radius: 30rpx;
    background: linear-gradient(90deg, #DE8E6A 0%, #FFC0A1 32%, #FFC0A1 68%, #DE8E6A 100%);
    position: absolute;
    bottom: 30rpx;
    right: 40rpx;
    text-align: center;
    line-height: 60rpx;
    font-size: 24rpx;
  }

  .buts {
    width: 160rpx;
    height: 60rpx;
    border-radius: 30rpx;
    background-color: rgb(175, 175, 175);
    color: white;
    font-weight: bold;
    position: absolute;
    bottom: 30rpx;
    right: 40rpx;
    text-align: center;
    line-height: 60rpx;
    font-size: 24rpx;
  }
  
  /* 主体 */
  .memberBody {
    width: 90%;
    height: 800rpx;
    box-shadow: 0px -1px 10px rgba(206, 206, 206, 0.41);
    border-radius: 25rpx;
    margin: auto;
  }
  
  .memberBody>.m-title {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    font-size: 30rpx;
    font-weight: bold;
    position: relative;
  }
  
  .memberBody>.m-title>image {
    width: 350rpx;
    height: 50rpx;
    position: absolute;
    top: 22rpx;
    left: 0;
    right: 0;
    margin: auto;
    z-index: -1;
  }
  
  .memberBody>.m-body {
    margin: 10rpx auto;
    width: 100%;
    display: flex;
    justify-content: start;
    flex-wrap: wrap;
    padding: 20rpx;
  }
  
  .memberBody>.m-body>.m-box {
    width: 33%;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    margin-bottom: 80rpx;
  }
  
  .memberBody>.m-body>.m-box>.top {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    background: linear-gradient(90deg, #DE8E6A 0%, #FFC0A1 33%, #FFC0A1 68%, #DE8E6A 100%);
    display: flex;
    justify-content: center;
    align-items: center;
  }
  .memberBody>.m-body>.m-box>.top>image{
    width: 52rpx;
    height: 52rpx;
  }
  
  .memberBody>.m-body>.m-box>.bottom{
    font-size: 22rpx;
    text-align: center;
    padding-top: 16rpx;
    padding-bottom: 40rpx;
    color: #46291F;
    height: 40rpx;
  }
  
  .padBot{
    padding-bottom: 200rpx;
    color: #8f8f8f;
  }




  /* 免责声明 */
.Tips {
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  background-color: #707070;
  z-index: 9999;
}

.Tips>.t-box {
  width: 580rpx;
  height: 560rpx;
  border-radius: 40rpx;
  position: absolute;
  top: 25vh;
  left: 0;
  right: 0;
  margin: auto;
  background-color: white;
}

.Tips>.t-box>.t-top {
  width: 100%;
  text-align: center;
  line-height: 100rpx;
  font-size: 34rpx;
  font-weight: bold;
  border-bottom: 1rpx solid gainsboro;
}

.Tips>.t-box>.t-body {
  width: 90%;
  padding: 40rpx 30rpx;
  color: #646464;
  font-size: 22rpx;
  line-height: 40rpx;
  text-align: start;
  margin: auto;
}

.Tips>.t-box>.t-bottom {
  width: 520rpx;
  height: 80rpx;
  font-size: 32rpx;
  font-weight: bold;
  line-height: 80rpx;
  color: #46291F;
  background: linear-gradient(90deg, #DE8E6A 0%, #FFC0A1 32%, #FFC0A1 68%, #DE8E6A 100%);
  margin: auto;
  border-radius: 55rpx;
  text-align: center;
}
