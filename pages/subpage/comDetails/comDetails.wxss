page {
    background-color: #FCFBFB;
}

button::after {
    border: none !important;
    padding: 0 !important;
    margin: 0 !important;
}

button {
    background-color: transparent !important;
    padding: 0 !important;
    line-height: inherit !important;
    margin: 0 !important;
    font-weight: 500 !important;
    border-radius: none !important;
}

/* 顶部商品详情 */
.topCard {
    width: 100%;
    margin: 20rpx auto;
    border-radius: 20rpx;
    overflow: hidden;
    background-color: white;
    box-shadow: 0px 2px 4px rgba(178, 175, 175, 0.10);
}

.swiper {
    width: 100%;
    height: 300rpx;
}

.swiper-img {
    width: 100%;
    height: 100%;
}


.cardMsg {
    width: 100%;
    padding: 20rpx;
}

.cardMsg>.c-box1 {
    font-size: 26rpx;
    font-weight: bold;
    color: #030303;
}

.cardMsg>.c-box2 {
    font-size: 22rpx;
    font-weight: bold;
    color: #46291f91;
    line-height: 60rpx;
}

.cardMsg>.c-box3 {
    font-size: 24rpx;
    font-weight: bold;
    display: flex;
    justify-content: space-between;
    align-items: center;
}


/* 主体详情 */
.bodyCard {
    width: 100%;
    margin: 30rpx auto;
    border-radius: 20rpx;
    overflow: hidden;
    background-color: white;
    box-shadow: 0px 2px 4px rgba(178, 175, 175, 0.10);
}

.bodyCard>.b-title {
    width: 100%;
    padding: 20rpx;
    display: flex;
    justify-content: start;
    align-items: center;
}

.bodyCard>.b-title>view {
    margin: 0 10rpx;
}

.bodyCard>.b-title>.bt1 {
    border-radius: 6rpx;
    width: 8rpx;
    height: 30rpx;
    background: linear-gradient(180deg, #DE8E6A 0%, #FFC0A1 32%, #FFC0A1 68%, #DE8E6A 100%);
}

.bodyCard>.b-title>.bt2 {
    color: #46291F;
    font-size: 30rpx;
    font-weight: bold;
}

.bodyCard>.b-title>.bt3 {
    color: #030303;
    font-size: 22rpx;
    font-weight: bold;
}

.bodyCard>.b-body {
    padding: 10rpx 20rpx 20rpx 20rpx;
    display: flex;
    justify-content: center;
}



/* 评论模块 */
.comment {
    width: 100%;
    margin: 30rpx auto;
    border-radius: 20rpx;
    overflow: hidden;
    background-color: white;
    box-shadow: 0px 2px 4px rgba(178, 175, 175, 0.10);
    padding-bottom: 40rpx;
}

.comment>.com-title {
    width: 100%;
    padding: 20rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.comment>.com-title>.c-left {
    display: flex;
    justify-content: start;
    align-items: center;
}

.comment>.com-title>.c-left>view {
    margin: 0 10rpx;
}

.comment>.com-title>.c-left>.cl1 {
    border-radius: 6rpx;
    width: 8rpx;
    height: 30rpx;
    background: linear-gradient(180deg, #DE8E6A 0%, #FFC0A1 32%, #FFC0A1 68%, #DE8E6A 100%);
}

.comment>.com-title>.c-left>.cl2 {
    font-size: 28rpx;
    color: #46291F;
    font-weight: bold;
}

.comment>.com-title>.c-right {
    font-size: 20rpx;
    font-weight: bold;
    color: #46291F;
}


.comment>.com-body {
    width: 100%;
    display: flex;
    flex-direction: column;
}


.comment>.com-body>.cb-top {
    width: 100%;
    padding: 20rpx;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.comment>.com-body>.cb-top>.ct1>image {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50rpx;
}

.comment>.com-body>.cb-top>.ct2 {
    margin-left: 30rpx;
    display: flex;
    justify-content: start;
    align-items: flex-start;
    flex-direction: column;
}

.comment>.com-body>.cb-top>.ct2>view {
    color: #46291F;
    font-size: 26rpx;
    font-weight: bold;
}


.comment>.com-body>.cb-bottom {
    width: 95%;
    margin: 0rpx auto;
    padding: 20rpx;
    border-bottom: 1rpx solid #de8f6a79;
}

.comment>.com-body>.cb-bottom>image {
    width: 100rpx;
    height: 100rpx;
    border-radius: 10rpx;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
}

.comment>.com-body>.cb-bottom>view {
    color: #DE8E6A;
    font-size: 24rpx;
    font-weight: bold;
}


/* 底部购买框 */
.bottom {
    width: 100%;
    height: 110rpx;
    background-color: white;
    position: fixed;
    bottom: 0;
    left: 0;
    display: flex;
    justify-content: space-between;
}

.bottom>.bottomLeft {
    width: 50%;
    height: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.bottom>.bottomLeft>.bl-box {
    width: 50%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}

.bottom>.bottomLeft>.xian {
    width: 0;
    height: 60rpx;
    border: 1rpx solid #707070;
}

.bottom>.bottomLeft>.bl-box>image {
    width: 40rpx;
    height: 40rpx;
}

.bottom>.bottomRight {
    width: 50%;
    height: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.bottom>.bottomRight>.br-box1 {
    width: 50%;
    height: 100%;
    text-align: center;
    line-height: 100rpx;
    background-color: #46291f73;
    color: white;
    font-size: 30rpx;
}

.bottom>.bottomRight>.br-box2 {
    width: 100%;
    height: 100%;
    text-align: center;
    line-height: 100rpx;
    background: linear-gradient(91deg, #FF9966 0%, #FFC0A1 32%, #FFC0A1 68%, #FFA982 100%);
    color: #46291F;
    font-size: 30rpx;
    font-weight: bold;
}


/* 选择规格 */
.choice {
    width: 100vw;
    height: 100vh;
    background-color: rgba(51, 51, 51, 0.733);
    z-index: 10;
    position: fixed;
    top: 0;
    left: 0;
}

.choice>.choice-box {
    width: 95%;
    /* height: 70vh; */
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    background-color: white;
    border-radius: 20rpx 20rpx 0 0;
    overflow: hidden;
}

.choice>.choice-box>.swiper {
    background-color: white;
}

.choice>.choice-box>.box1 {
    width: 100%;
    padding: 20rpx;
    display: flex;
    justify-content: start;
    align-items: flex-start;
    flex-direction: column;
    border-bottom: 1rpx solid #46291f33;
}

.choice>.choice-box>.box1>.m1-box1 {
    font-size: 26rpx;
    font-weight: bold;
    color: #030303;
}

.choice>.choice-box>.box1>.m1-box2 {
    font-size: 20rpx;
    color: #46291fa8;
    line-height: 60rpx;
}

.choice>.choice-box>.box1>.m1-box3 {
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.choice>.choice-box>.box2 {
    display: flex;
    flex-direction: column;
    justify-content: center;
    padding: 20rpx;
}

.choice>.choice-box>.box2>.m2-box1 {
    width: 100%;
    font-size: 30rpx;
    font-weight: bold;
    color: #46291F;
}

.choice>.choice-box>.box2>.m2-box2 {
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
    flex-wrap: wrap;
    margin-top: 20rpx;
    padding-bottom: 20rpx;
    border-bottom: 1rpx solid #46291f50;
    font-size: 22rpx;
}

.choice>.choice-box>.box2>.m2-box2>.box {
    width: 315rpx;
    border-radius: 5rpx;
    background-color: #e9e9e9;
    text-align: center;
    overflow: hidden;
    color: #46291F;
    margin: 10rpx;
    padding: 15rpx;
}

.choice>.choice-box>.box2>.m2-box2>.box2 {
    width: 315rpx;
    border-radius: 5rpx;
    background-color: #e9e9e9;
    text-align: center;
    overflow: hidden;
    color: #DE8E6A;
    border: 1rpx solid #DE8E6A;
    margin: 10rpx;
    padding: 15rpx;
}

.choice>.choice-box>.box3 {
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    padding: 40rpx;
}

.choice>.choice-box>.box3>.m3-box1 {
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-bottom: 100rpx;
}

.choice>.choice-box>.box3>.m3-box1>.box1 {
    color: #46291F;
    font-size: 26rpx;
    font-weight: bold;
}

.choice>.choice-box>.box3>.m3-box1>.box2 {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 120rpx;
    font-size: 24rpx;
}

.choice>.choice-box>.box3>.m3-box1>.box2>view {
    width: 32rpx;
    height: 32rpx;
    border-radius: 50%;
    text-align: center;
    line-height: 32rpx;
    background-color: #F3F3F3;
}

.choice>.choice-box>.box3>.m3-box2 {
    width: 460rpx;
    height: 70rpx;
    text-align: center;
    line-height: 70rpx;
    color: #46291F;
    border-radius: 40rpx;
    /* position: fixed;
    left: 0;
    right: 0;
    bottom: 40rpx; */
    margin: auto;
    background: linear-gradient(85deg, #FF9966 0%, #FFC0A1 32%, #FFC0A1 68%, #FFA982 100%);
}




